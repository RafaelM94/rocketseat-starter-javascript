2º Exercicio:

<html>
    <head>
        <title>Exercicio 2</title>
        <meta charset="UTF-8">
    </head>
    <body>

        <script>

            var par = [];
            
            function pares(x, y) {
                for (x; x<=y; x++){
                    if((x%2)==0){
                        par.push(x);
                    }
                }                
            };
            pares(32,321)
            console.log(par);     
       
        </script>
    <body>
</html>

3º Exercicio:

<html>
    <head>
        <title>Exercicio 3</title>
        <meta charset="UTF-8">
    </head>
    <body>

        <script>
           function temHabilidade(skills){                             
               if(skills.indexOf('Javascript')){
                   return false;
               } else{
                   return true;
               }
           }
           var skills = ['Javascript', 'ReactJS', 'React Native'];           
           console.log(temHabilidade(skills));
        </script>
    <body>
</html>

4º Exercicio:

<html>
    <head>
        <title>Exercicio 4</title>
        <meta charset="UTF-8">
    </head>
    <body>

        <script>        

            function experiencia(anos){
                switch(true){
                    case anos >= 0 && anos <= 1:
                        return 'iniciante';   
                    break;   

                    case anos > 1 && anos <= 3:
                        return 'Intermediário';
                    break;

                    case anos > 3 && anos <= 6:
                        return 'Avançado'; 
                    break;  

                    case anos >= 7:
                        return 'Jedi Master';
                    break; 

                    default:
                        return 'Ano invalido';
                }                
            }   
            
            var anoEstudo = 7;
            console.log(experiencia(anoEstudo));

            // De 0-1 ano: Iniciante
            // De 1-3 anos: Intermediário
            // De 3-6 anos: Avançado
            // De 7 acima: Jedi Master

        </script>
    <body>
</html>

