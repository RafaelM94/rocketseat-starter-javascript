           função temHabilidade (habilidades) {                             
               if (skills.indexOf ('Javascript')) {
                   retorna falso;
               } outro{
                   return true;
               }
           }
           var skills = ['Javascript', 'ReactJS', 'React Native'];           
           console.log (temHabilidade (habilidades));
        </script>
