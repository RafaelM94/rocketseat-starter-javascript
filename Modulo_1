4º Exercício:

<html>
    <head>
        <title> Exercício 4 </title>
        <meta charset = "UTF-8">
    </head>
    <body>

        <script>        

            função experiencia (anos) {
                switch (true) {
                    case anos> = 0 && anos <= 1:
                        retornar 'iniciante';   
                    pausa;   

                    case anos> 1 && anos <= 3:
                        retornar 'Intermediário';
                    pausa;

                    case anos> 3 && anos <= 6:
                        retornar 'Avançado'; 
                    pausa;  

                    caso anos> = 7:
                        retornar 'Mestre Jedi';
                    pausa; 

                    padrão:
                        retornar 'Ano invalido';
                }                
            }   
            
            var anoEstudo = 7;
            console.log (experiência (anoEstudo));

            // De 0-1 ano: Iniciante
            // De 1-3 anos: Intermediário
            // De 3-6 anos: Avançado
            // De 7 acima: Mestre Jedi

        </script>
    <body>
</html>

5º Exercício:

<html>
    <head>
        <title> Exercício 5 </title>
        <meta charset = "UTF-8">
    </head>
    <body>

        <script>  

            var usuarios = [
                {nome: "Diego",
                    habilidades: ["Javascript", "ReactJS", "Redux"]
                }
                {nome: "Gabriel",
                    habilidades: ["VueJS", "Ruby on Rails", "Elixir"]
                }
            ];
            
           usuários da função (usuarios) {
                for (deixe usuários de usuarios) {
                    console.log ('O' + users.nome + 'possui as habilidades' + users.habilidades + '.');
                }
            }

            usuários (usuarios);
           
        </script>
    <body>
</html>
